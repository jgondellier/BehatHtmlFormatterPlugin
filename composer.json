{
    "name": "gondellier/behat-html-formatter",
    "description": "This will create a html formatter for Behat.",
    "authors": [
        {
            "name": "Neal Vanmeert et un tout petit peu moi"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "twig/twig":"~2.0",
        "behat/behat": "~3.0",
        "behat/gherkin": "~4.2",
        "ext-json": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/process": ">2.3,<4.0",
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "gondellier\\BehatHTMLFormatter\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master@dev"
        }
    }
}
